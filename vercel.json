{
  "version": 2,
  "builds": [
    {
      "src": "dist/index.js", 
      "use": "@vercel/node"
    }
  ],
  "rewrites": [
    {
      "source": "/routes/(.*)",
      "destination": "/dist/routes/Routes.js"
    }
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    }
  ]
}
